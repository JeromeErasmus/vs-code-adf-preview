{
  "ADF Info Panel": {
    "prefix": "info",
    "body": [
      "> ℹ️ **Info:** $1"
    ],
    "description": "Insert ADF info panel in markdown"
  },
  "ADF Warning Panel": {
    "prefix": "warn",
    "body": [
      "> ⚠️ **Warning:** $1"
    ],
    "description": "Insert ADF warning panel in markdown"
  },
  "ADF Success Panel": {
    "prefix": "success",
    "body": [
      "> ✅ **Success:** $1"
    ],
    "description": "Insert ADF success panel in markdown"
  },
  "ADF Note Panel": {
    "prefix": "note",
    "body": [
      "> 📝 **Note:** $1"
    ],
    "description": "Insert ADF note panel in markdown"
  },
  "ADF Error Panel": {
    "prefix": "error",
    "body": [
      "> ❌ **Error:** $1"
    ],
    "description": "Insert ADF error panel in markdown"
  },
  "ADF Task List": {
    "prefix": "tasks",
    "body": [
      "- [ ] $1",
      "- [ ] $2",
      "- [ ] $3"
    ],
    "description": "Insert ADF task list items"
  },
  "ADF Table": {
    "prefix": "table",
    "body": [
      "| ${1:Header 1} | ${2:Header 2} | ${3:Header 3} |",
      "|--------------|--------------|--------------|",
      "| ${4:Cell 1}   | ${5:Cell 2}   | ${6:Cell 3}   |",
      "| ${7:Cell 4}   | ${8:Cell 5}   | ${9:Cell 6}   |"
    ],
    "description": "Insert ADF compatible table"
  },
  "ADF Meeting Notes Template": {
    "prefix": "meeting",
    "body": [
      "# ${1:Meeting Title}",
      "",
      "> ℹ️ **Info:**",
      "> Date: ${2:$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE}",
      "> Attendees: ${3:attendees}",
      "",
      "## Agenda",
      "- ${4:Item 1}",
      "- ${5:Item 2}",
      "",
      "## Discussion",
      "${6:discussion notes}",
      "",
      "## Action Items",
      "- [ ] ${7:Action 1}",
      "- [ ] ${8:Action 2}",
      "",
      "## Next Steps",
      "${9:next steps}"
    ],
    "description": "Insert meeting notes template with ADF elements"
  }
}