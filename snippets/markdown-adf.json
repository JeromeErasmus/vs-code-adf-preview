{
  "ADF Info Panel": {
    "prefix": ["info", "panel-info"],
    "body": [
      "~~~panel type=info title=\"${1:Information Panel}\"",
      "$2",
      "~~~"
    ],
    "description": "Insert ADF info panel in markdown"
  },
  "ADF Warning Panel": {
    "prefix": ["warn", "panel-warn", "panel-warning"],
    "body": [
      "~~~panel type=warning title=\"${1:Warning Panel}\"",
      "$2",
      "~~~"
    ],
    "description": "Insert ADF warning panel in markdown"
  },
  "ADF Success Panel": {
    "prefix": ["success", "panel-success"],
    "body": [
      "~~~panel type=success title=\"${1:Success Panel}\"",
      "$2",
      "~~~"
    ],
    "description": "Insert ADF success panel in markdown"
  },
  "ADF Note Panel": {
    "prefix": ["note", "panel-note"],
    "body": [
      "~~~panel type=note title=\"${1:Note Panel}\"",
      "$2",
      "~~~"
    ],
    "description": "Insert ADF note panel in markdown"
  },
  "ADF Error Panel": {
    "prefix": ["error", "panel-error"],
    "body": [
      "~~~panel type=error title=\"${1:Error Panel}\"",
      "$2",
      "~~~"
    ],
    "description": "Insert ADF error panel in markdown"
  },
  "ADF Task List": {
    "prefix": "tasks",
    "body": [
      "- [ ] $1",
      "- [ ] $2",
      "- [ ] $3"
    ],
    "description": "Insert ADF task list items"
  },
  "ADF Table": {
    "prefix": "table",
    "body": [
      "| ${1:Header 1} | ${2:Header 2} | ${3:Header 3} |",
      "|--------------|--------------|--------------|",
      "| ${4:Cell 1}   | ${5:Cell 2}   | ${6:Cell 3}   |",
      "| ${7:Cell 4}   | ${8:Cell 5}   | ${9:Cell 6}   |"
    ],
    "description": "Insert ADF compatible table"
  },
  "ADF Meeting Notes Template": {
    "prefix": "meeting",
    "body": [
      "# ${1:Meeting Title}",
      "",
      "~~~panel type=info title=\"Meeting Details\"",
      "Date: ${2:$CURRENT_YEAR-$CURRENT_MONTH-$CURRENT_DATE}",
      "Attendees: ${3:attendees}",
      "~~~",
      "",
      "## Agenda",
      "- ${4:Item 1}",
      "- ${5:Item 2}",
      "",
      "## Discussion",
      "${6:discussion notes}",
      "",
      "## Action Items",
      "- [ ] ${7:Action 1}",
      "- [ ] ${8:Action 2}",
      "",
      "## Next Steps",
      "${9:next steps}"
    ],
    "description": "Insert meeting notes template with ADF elements"
  },
  "ADF Expand Section": {
    "prefix": ["expand", "collapsible"],
    "body": [
      "~~~expand title=\"${1:Click to expand}\" expanded=${2:false}",
      "$3",
      "~~~"
    ],
    "description": "Insert ADF expandable/collapsible section"
  }
}